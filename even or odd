//check odd or even
#include <stdio.h>

int main()
{
    int num;
    printf("***Odd or Even***\n");
    printf("enter a number:");
    scanf("%d",&num);
    
    if(num%2==0)
        printf("the entered number %d is even",num);
    else
        printf("the entered number %d is odd",num);

    return 0;
}

